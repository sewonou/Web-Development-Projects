<%- include("header") %>
    <div class="box">
        <% for(let i = 0 ; i < newItems.length; i++ ) { %>
            <div class="item">
                <input type="checkbox">
                <p><%= newItems[i]%></p>
            </div>
        <% } %>
        <form action="/" method="post" class="item">
            <input  type="text" name="newItem" id="todo" placeholder="Add a new action" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>

    </div>
<%- include("footer") %>
